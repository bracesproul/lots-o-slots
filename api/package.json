    "jest-extended": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.kebabcase": "^4.1.1",
    "lodash.snakecase": "^4.1.1",
    "mailparser": "^3.6.4",
    "mockdate": "^3.0.5",
    "node-imap": "^0.9.6",
    "patch-package": "^6.5.1",
    "pg": "^8.8.0",
    "prettier": "^2.7.1",
    "randomatic": "^3.1.1",
    "string-strip-html": "8.4.0",
    "terminal-link": "^2.1.1",
    "ts-jest": "^27.0.4",
    "ts-morph": "^14.0.0",
    "ts-node": "^10.0.0",
    "tsc-alias": "^1.5.0",
    "tsc-watch": "^4.4.0",
    "tsconfig-paths": "^3.12.0",
    "type-graphql": "^1.1.1",
