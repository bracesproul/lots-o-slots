    "cdktf": "^0.8.6",
    "cdktf-cli": "^0.8.6",
    "chalk": "4.1.0",
    "class-validator": "^0.14.0",
    "constructs": "^10.0.29",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "date-fns": "^2.28.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.6.0",
    "faker": "^5.5.3",
    "fishery": "^2.2.2",
    "googleapis": "^109.0.1",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.10.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.6.0",
