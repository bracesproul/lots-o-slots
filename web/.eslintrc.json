{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react"],
  "env": {
    "es2021": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "window": true,
    "process": true
  },
  "extends": [
    "plugin:react-hooks/recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}
